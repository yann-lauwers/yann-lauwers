{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "start": {
      "env": ["DATABASE_URL", "NEXT_PUBLIC_TEST_ENV"],
      "dependsOn": ["^build"]
    },
    "build": {
      "outputs": [".next/**", "!.next/cache/**"],
      "env": ["DATABASE_URL", "NEXT_PUBLIC_TEST_ENV"],
      "dependsOn": ["^build"]
    },
    "clean": {
      "cache": false
    }
  }
}
